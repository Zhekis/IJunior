using System;

class Program
{
    static void Main(string[] args)
    {
        int allPic = 52;
        int numPicsInRow = 3;
        int fullCountRow = allPic / numPicsInRow;
        int countPicsOver = allPic % numPicsInRow;
        Console.WriteLine(fullCountRow);
        Console.WriteLine(countPicsOver);
    }
}
