using System;

class Program
{

    static void Main(string[] args)
    {    
        Random rand = new Random();
        int number = rand.Next(0,101);
        int sum = 0;

        for (int i = 0; i <= number; i++)
        {
            if (i % 3 == 0)
                sum += i;
        }
        Console.WriteLine(sum);
    }
}
