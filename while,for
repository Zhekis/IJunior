using System;

class Program
{
    static void Main(string[] args)
    {    
        Console.WriteLine("How many times do you want to say 'Happy Birthday'");
        string phrase = "Happy Birthday!";
        int countCelebrates = Convert.ToInt32(Console.ReadLine());
        for (int i = 0; i < countCelebrates; i++)
        {
            Console.WriteLine(phrase);
        }

    }
}
